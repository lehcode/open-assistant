    "graph": "nx graph",
    "help": "nx help",
    "prelint": "nx reset",
    "lint": "nx run-many --target=lint --projects=admin,api",
    "nx": "nx",
    "start": "nx run-many --target=serve:development --projects=admin,api",
    "start-admin": "nx run admin:serve:development",
    "start-api": "nx run api:serve:development",
    "pretest": "nx reset",
    "test": "nx run-many --target=test --projects=admin,api --configuration=testing",
    "test-admin": "nx run admin:test:testing",
    "test-api": "nx run api:test:testing"
