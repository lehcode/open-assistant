    "affected": "nx affected",
    "prebuild": "nx run shared:build:production",
    "build": "nx run-many --target=build --projects=admin,api --configuration=production",
    "delete-project": "npx nx generate @nx/workspace:remove",
    "docs-api": "npx @compodoc/compodoc -p api/tsconfig.json -s",
    "pree2e": "nx reset",
