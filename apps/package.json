    "react-refresh": "^0.10.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.0",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.format:write": "prettier --write"
  },
  "packageManager": "yarn@1.22.22"
}
