{
  "name": "open-assistant",
  "version": "0.0.1",
  "description": "Personal Assistant",
  "license": "MIT",
  "scripts": {
    "test": "nx run-many --target=test --projects=email,api --parallel=true",
    "lint": "nx run-many --target=lint --projects=email,api --parallel=true",
    "e2e": "nx e2e",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "graph": "nx graph",
    "help": "nx help",
    "nx": "nx",
    "build": "nx run-many --target=build --projects=email,api --parallel=true --output-style=static --skipNxCache",
    "build-api": "nx run api:build --skipNxCache",
    "build-ui": "nx run email:build --skipNxCache",
    "start-api": "nx run api:serve --verbose",
    "start-ui": "nx run email:serve --verbose --host=0.0.0.0",
    "start": "nx run-many --verbose --target=serve --projects=email,api --parallel=true",
    "test-api": "nx run api:test",
    "test-ui": "nx run email:test",
    "docs-api": "npx @compodoc/compodoc -p api/tsconfig.json -s",
    "prepare": "husky",
    "delete-project": "npx nx generate @nx/workspace:remove"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "axios": "^1.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/angular": "^20.0.3",
    "@nx/devkit": "20.0.2",
    "@nx/eslint": "20.0.2",
    "@nx/eslint-plugin": "20.0.2",
    "@nx/jest": "20.0.2",
    "@nx/js": "20.0.3",
    "@nx/nest": "^20.0.2",
    "@nx/node": "20.0.2",
    "@nx/playwright": "20.0.2",
    "@nx/react": "^20.0.2",
    "@nx/vite": "20.0.3",
    "@nx/vue": "^20.0.3",
    "@nx/web": "20.0.3",
    "@nx/webpack": "20.0.2",
    "@playwright/test": "^1.36.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/react": "15.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.7.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": ">=6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsdom": "~22.1.0",
    "lint-staged": ">=10",
    "nx": "20.0.2",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.0",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.format:write": "prettier --write"
  },
  "packageManager": "yarn@1.22.22"
}
