{
  "name": "open-assistant",
  "version": "0.0.1",
  "description": "Personal Assistant",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "test": "nx reset && nx run-many --target=test --projects=email,api --parallel=true",
    "lint": "nx reset && nx run-many --target=lint --projects=email,api --parallel=true",
    "e2e": "nx reset && nx e2e",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "graph": "nx reset && nx graph",
    "help": "nx help",
    "nx": "nx",
    "prebuild": "nx reset && nx run-many --target=build --projects=types,interfaces --parallel=true --output-style=static",
    "build": "yarn run build-libs && nx run-many --target=build --projects=email,api --parallel=true --output-style=static",
    "build-api": "nx reset && nx run api:build",
    "build-email": "nx reset && nx run email:build",
    "build-libs": "nx reset && run-many --target=build --projects=types,interfaces --parallel=true --output-style=static",
    "start-api": "nx reset && VITE_CJS_IGNORE_WARNING=true nx run api:serve:development",
    "start-email": "nx reset && nx run email:serve:development",
    "start": "nx reset && nx run-many --target=serve:development --projects=email,api --parallel=true",
    "test-api": "nx run api:test",
    "test-email": "nx run email:test",
    "docs-api": "npx @compodoc/compodoc -p api/tsconfig.json -s",
    "prepare": "husky",
    "delete-project": "npx nx generate @nx/workspace:remove"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@hookform/resolvers": "^3.9.1",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.2",
    "axios": "^1.6.0",
    "express-session": "^1.18.1",
    "lucide-react": "^0.454.0",
    "mobx": "^6.13.5",
    "mobx-react": "^9.1.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.53.1",
    "react-router-dom": "^6.27.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/preset-react": "^7.14.5",
    "@eslint/compat": "^1.2.1",
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/angular": "^20.0.3",
    "@nx/devkit": "20.0.2",
    "@nx/eslint": "20.0.2",
    "@nx/eslint-plugin": "^20.0.6",
    "@nx/jest": "20.0.2",
    "@nx/js": "20.0.3",
    "@nx/nest": "^20.0.2",
    "@nx/node": "20.0.2",
    "@nx/playwright": "20.0.2",
    "@nx/react": "^20.0.2",
    "@nx/vite": "20.0.3",
    "@nx/vue": "^20.0.3",
    "@nx/web": "20.0.3",
    "@nx/webpack": "20.0.2",
    "@playwright/test": "^1.36.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/react": "15.0.6",
    "@types/eslint__js": "^8.42.3",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.12",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "@vue/eslint-config-prettier": "7.1.0",
