{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/out-tsc",
    "types": [
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ],
    "composite": true,
    "inlineSourceMap": true
  },
  "exclude": [
    "jest.config.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.tsx",
    "src/**/*.test.tsx",
    "vite.config.ts",
    "vitest.config.ts"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "../libs/feature/**/*.ts",
  ]
}
