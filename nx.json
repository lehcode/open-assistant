{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "master",
  "namedInputs": {
    "default": [
      "{projectRoot}/apps/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default",
      "!{projectRoot}/apps/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/apps/tsconfig.spec.json",
      "!{projectRoot}/apps/.eslintrc.json",
      "!{projectRoot}/apps/eslint.config.cjs",
      "!{projectRoot}/apps/jest.config.[jt]s",
      "!{projectRoot}/apps/src/test-setup.[jt]s",
      "!{projectRoot}/apps/test-setup.[jt]s"
    ],
    "development": [
      "default",
      "{projectRoot}/apps/src/**/*"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/.gitlab-ci.yml"
    ]
  },
  "plugins": [
    {
      "plugin": "@nx/js/typescript",
      "options": {
        "typecheck": {
          "targetName": "typecheck"
        },
        "build": {
          "targetName": "build",
          "configName": "tsconfig.lib.json"
        }
      },
      "exclude": [
        "apps/libs/**/*"
      ]
    },
    {
      "plugin": "@nx/webpack/plugin",
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "serve",
        "previewTargetName": "preview"
      }
    },
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "buildTargetName": "build",
        "testTargetName": "test",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "serveStaticTargetName": "serve-static",
        "typecheckTargetName": "typecheck"
      }
    },
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    },
    {
      "plugin": "@nx/playwright/plugin",
      "options": {
        "targetName": "e2e"
      },
      "exclude": [
        "apps/**/*.spec.ts",
        "apps/**/*.spec.tsx"
      ]
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test"
      },
      "exclude": [
        "apps/**/*-e2e/**/*"
      ]
    },
    {
      "plugin": "@nx/js/typescript",
      "include": [
        "apps/libs/shared/*"
      ],
      "options": {
        "typecheck": {
          "targetName": "typecheck"
        },
        "build": {
          "targetName": "build",
          "configName": "tsconfig.lib.json"
        }
      }
    }
  ],
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ],
      "cache": true
    },
    "serve": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "development",
        "^development"
      ],
      "cache": true
    },
    "e2e-ci--**/*": {
      "dependsOn": [
        "^build"
      ]
    },
    "@angular-devkit/build-angular:browser": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "@nx/js:tsc": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ],
      "options": {}
    },
    "@nx/vite:build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    }
  },
  "generators": {
    "@nx/react": {
      "application": {
        "babel": true,
        "style": "tailwind",
        "linter": "eslint",
        "bundler": "webpack"
      },
      "component": {
        "style": "tailwind"
      },
      "library": {
        "style": "tailwind",
        "linter": "eslint"
      }
    },
    "@nx/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "playwright"
    },
    "@nx/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nx/angular": {
      "convert-tslint-to-eslint": {
        "removeTSLintIfNoMoreTSLintTargets": true
      }
    }
  },
  "defaultProject": "api",
  "parallel": 1,
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "useLockFile": false,
        "skipNxCache": true
      }
    }
  },
  "release": {
    "version": {
      "preVersionCommand": "npx nx run-many -t build"
    }
  },
  "nxCloudId": "673569bc4d13b0163df75276"
}
